{
  "name": "@effectful/debugger",
  "version": "2.0.4",
  "description": "User space debugging",
  "main": "vscode.js",
  "repository": "https://github.com/awto/effectfuljs.git",
  "author": "Vitalii Akimov <vitaliy.akimov@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "eslint src && jest",
    "build:transform": "babel src/transform.js --out-file transform.js",
    "build:decls": "tsc",
    "build": "npm run build:transform && npm run build:decls",
    "build:rt": "babel --extensions \".ts,.js\" --out-dir . src",
    "build:deps": "pnpify node ./buildDeps",
    "build-loose": "npm run build:rt && npm run build:deps",
    "eslint": "eslint src",
    "link-test": "node ./test/makeLinks",
    "docs": "rimraf api && typedoc --plugin typedoc-plugin-markdown --out api --excludePrivate src --readme none"
  },
  "browser": {
    "vm": false,
    "worker_threads": false,
    "ws": false
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/types": "^7.27.3",
    "@types/asap": "^2.0.2",
    "@types/babel-types": "^7.0.16",
    "@types/babel__generator": "^7.27.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.29",
    "@yarnpkg/pnpify": "^4.1.5",
    "assert": "^2.1.0",
    "babel-loader": "^10.0.0",
    "core-js-pure": "^3.42.0",
    "eslint": "^9.28.0",
    "events": "^3.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "lodash": "^4.17.21",
    "lodash.sortby": "^4.7.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.5.3",
    "prop-types": "^15.8.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@babel/core": "^7.27.4",
    "@babel/generator": "^7.27.5",
    "@babel/parser": "^7.27.5",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-function-sent": "^7.27.1",
    "@babel/plugin-transform-arrow-functions": "^7.27.1",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-class-static-block": "^7.27.1",
    "@babel/plugin-transform-classes": "^7.27.1",
    "@babel/plugin-transform-destructuring": "^7.27.3",
    "@babel/plugin-transform-dynamic-import": "^7.27.1",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/plugin-transform-logical-assignment-operators": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.27.1",
    "@babel/plugin-transform-numeric-separator": "^7.27.1",
    "@babel/plugin-transform-object-rest-spread": "^7.27.3",
    "@babel/plugin-transform-optional-catch-binding": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.27.1",
    "@babel/plugin-transform-parameters": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-property-mutators": "^7.27.1",
    "@babel/plugin-transform-react-display-name": "^7.27.1",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/plugin-transform-shorthand-properties": "^7.27.1",
    "@babel/plugin-transform-spread": "^7.27.1",
    "@babel/plugin-transform-template-literals": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.27.1",
    "@effectful/core": "workspace:^",
    "@effectful/serialization": "workspace:^",
    "@vscode/debugprotocol": "^1.68.0",
    "asap": "^2.0.6",
    "minimatch": "^10.0.1",
    "promise": "^8.3.0",
    "ws": "^8.18.2"
  },
  "preferUnplugged": true
}
